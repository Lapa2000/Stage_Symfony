                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for erp_commande_client in erp_commande_clients %}
            <tr>
                <td>{{ erp_commande_client.id }}</td>
                <td>{{ erp_commande_client.code }}</td>
                <td>{{ erp_commande_client.monlogo }}</td>
                <td>{{ erp_commande_client.masociete }}</td>
                <td>{{ erp_commande_client.monadresse }}</td>
                <td>{{ erp_commande_client.montel }}</td>
                <td>{{ erp_commande_client.nomclient }}</td>
                <td>{{ erp_commande_client.adresse }}</td>
                <td>{{ erp_commande_client.telclient }}</td>
                <td>{{ erp_commande_client.codedevis }}</td>
                <td>{{ erp_commande_client.datedevis }}</td>
                <td>{{ erp_commande_client.totalht }}</td>
                <td>{{ erp_commande_client.totaltva }}</td>
                <td>{{ erp_commande_client.totalttc }}</td>
                <td>{{ erp_commande_client.creepar }}</td>

                <td>{{ agent_fournisseur.nom }}</td>
                <td>{{ agent_fournisseur.prenom }}</td>
                <td>{{ agent_fournisseur.adresse }}</td>
                <td>{{ agent_fournisseur.tel }}</td>
                <td>{{ agent_fournisseur.tel2 }}</td>
                <td>{{ agent_fournisseur.fax }}</td>
                <td>{{ agent_fournisseur.email }}</td>
                <td>{{ agent_fournisseur.website }}</td>
                <td>{{ agent_fournisseur.image }}</td>
                <td>{{ agent_fournisseur.creepar }}</td>
